<TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="server">
    <variable>
        <name value="filter-identifier"/>
        <defaultValue value="urn:oid:2.16.840.1.113883.2.4.3.11.999.77.1.1|MBH_920_QA1"/>
        <description value="Filter on specific MedicationTreatment identifier"/>
    </variable>
    <nts:include value="mp9-ma-serve">
        <nts:with-parameter name="scenarioset" value="0"/>
        <nts:with-parameter name="scenario" value="7"/>
        <nts:with-parameter name="scenarioDescription" value="Medicatieafspraken met een specifieke medicamenteuze behandeling als filter (in kwalificatiesimulator het id van MBH_907_QA1 invoeren)"/>
        <nts:with-parameter name="scenarioPatient" value="B-XXX-Blik"/>
        <nts:with-parameter name="scenarioParams" value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900389&amp;category=http://snomed.info/sct|16076005&amp;pharmaceutical-treatment-identifier=${filter-identifier}&amp;_include=MedicationRequest:medication"/>
        <nts:with-parameter name="returnCount" value="1"/>
        <nts:with-parameter name="returnEntryCount" value="2"/>
        <nts:with-parameter name="returnEntryBreakdown" value="(Consists of at least 1 MedicationRequest and 1 Medication resources.)"/>
    </nts:include>
</TestScript>