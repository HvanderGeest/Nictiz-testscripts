<TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="server">
    <variable>
        <name value="filter-identifier"/>
        <defaultValue value="urn:oid:2.16.840.1.113883.2.4.3.11.999.77.6.1|MBH_920_QA1_MGB"/>
        <description value="Filter on specific MedicationUse identifier"/>
    </variable>
    <nts:include value="mp9.serve.mgb">
        <nts:with-parameter name="scenarioset" value="0"/>
        <nts:with-parameter name="scenario" value="2"/>
        <nts:with-parameter name="scenarioDescription" value="Specifieke medicatiegebruik met identificaties als filter (in kwalificatiesimulator het id van &quot;MBH_920_QA1_MGB&quot; invoeren) (Antwoord: bericht met 1 medicatiegebruik)"/>
        <nts:with-parameter name="scenarioPatient" value="B-XXX-Blik"/>
        <nts:with-parameter name="scenarioParams" value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900389&amp;category=http://snomed.info/sct|422979000&amp;identifier=${filter-identifier}&amp;_include=MedicationStatement:medication"/>
        <nts:with-parameter name="returnCount" value="1"/>
        <nts:with-parameter name="returnEntryCount" value="2"/>
        <nts:with-parameter name="returnEntryBreakdown" value="(Consists of 1 MedicationStatement and 1 Medication resources.)"/>
    </nts:include>
</TestScript>