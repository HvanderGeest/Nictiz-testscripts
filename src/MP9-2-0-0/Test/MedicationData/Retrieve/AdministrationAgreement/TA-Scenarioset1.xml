<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-AdministrationAgreement-retrieve-1-x"/>
   <version value="r4-mp9-2.0.0"/>
   <name value="Medication Process 9 2.0.0  - AdministrationAgreement - Retrieve - Scenario 1.x"/>
   <description value="Scenario 1.x - TEST Medicatiegegevens TA Scenarioset 1"/>
   <nts:includeDateT value="no"/>
   <test id="Scenario-1-x">
      <name value="Scenario 1.x"/>
      <description value="TEST Medicatiegegevens TA Scenarioset 1"/>
      <nts:include value="operation-search">
         <nts:with-parameter name="description"
                             value="Query MedicationDispense resource(s) for AdministrationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900419&amp;category=http://snomed.info/sct|422037009&amp;_include=MedicationDispense:medication"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="count" value="5"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="10"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 5 MedicationDispense and 5 Medication resources.)"/>
      </nts:include>
   </test>
</TestScript>
