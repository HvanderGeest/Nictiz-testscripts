<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-MedicationDispense-retrieve-2-x"/>
   <version value="r4-mp9-2.0.0"/>
   <name value="Medication Process 9 2.0.0  - MedicationDispense - Retrieve - Scenario 2.x"/>
   <description value="Scenario 2.x - TEST Medicatiegegevens MVE Scenarioset 2"/>
   <nts:includeDateT value="no"/>
   <test id="Scenario-2-x">
      <name value="Scenario 2.x"/>
      <description value="TEST Medicatiegegevens MVE Scenarioset 2"/>
      <nts:include value="operation-search">
         <nts:with-parameter name="description"
                             value="Query MedicationDispense resource(s) for MedicationDispense"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900420&amp;category=http://snomed.info/sct|373784005&amp;_include=MedicationDispense:medication"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="count" value="2"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="4"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 2 MedicationDispense and 2 Medication resources.)"/>
      </nts:include>
   </test>
</TestScript>
