<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <variable>
      <name value="filter-identifier"/>
      <defaultValue value="urn:oid:2.16.840.1.113883.2.4.3.11.999.77.52711000146108.1|MBH_200_QA1_VV"/>
      <description value="Filter on specific DispenseRequest identifier"/>
   </variable>
   <nts:include value="mp9-vv-serve">
      <nts:with-parameter name="scenarioset" value="0"/>
      <nts:with-parameter name="scenario" value="2"/>
      <nts:with-parameter name="scenarioDescription"
                          value="Specifieke verstrekkingsverzoek met identificaties als filter (in kwalificatiesimulator het id van &#34;MBH_200_QA1_VV&#34; invoeren) (Antwoord: bericht met 1 verstrekkingsverzoek)"/>
      <nts:with-parameter name="scenarioPatient" value="B-XXX-Blik"/>
      <nts:with-parameter name="scenarioParams"
                          value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900389&amp;category=http://snomed.info/sct|52711000146108&amp;identifier=${filter-identifier}&amp;_include=MedicationRequest:medication"/>
      <nts:with-parameter name="returnCount" value="1"/>
      <nts:with-parameter name="returnEntryCount" value="2"/>
      <nts:with-parameter name="returnEntryBreakdown"
                          value="(Consists of 1 MedicationRequest and 1 Medication resources.)"/>
   </nts:include>
</TestScript>
