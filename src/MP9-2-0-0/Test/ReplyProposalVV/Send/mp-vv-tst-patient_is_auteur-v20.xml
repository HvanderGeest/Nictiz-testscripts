<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-prescr-send--"/>
   <name value="MP9 - Client - Scenario . - Send Proposal dispense request (verstrekkingsverzoek)"/>
   <description value="Scenario . - Send Proposal dispense request (verstrekkingsverzoek) for C. van de XXX_Brugge."/>
   <nts:fixture id="mp-vv-tst-patient-is-auteur-v20"
                href="fixtures/mp-vv-tst-patient-is-auteur-v20.xml"/>
   <nts:includeDateT value="yes"/>
   <test id="scenario--send-proposal-vv">
      <name value="Scenario ."/>
      <description value="Send Proposal dispense request (verstrekkingsverzoek) in a transaction Bundle"/>
      <action>
         <operation>
            <type>
               <system value="http://hl7.org/fhir/testscript-operation-codes"/>
               <code value="transaction"/>
            </type>
            <description value="Test client to POST a Bundle of type transaction."/>
            <destination value="1"/>
            <origin value="1"/>
            <requestHeader>
               <field value="MP9-Request-ID"/>
               <value value="$"/>
            </requestHeader>
            <sourceId value="mp-vv-tst-patient-is-auteur-v20"/>
         </operation>
      </action>
      <nts:include value="assert.request.numResources"
                   scope="common"
                   resource="MedicationRequest"
                   count="1"/>
      <nts:include value="assert.request.numResources"
                   scope="common"
                   resource="Patient"
                   count="1"/>
      <nts:include value="assert.request.numResources"
                   scope="common"
                   resource="Organization"
                   count="1"/>
      <nts:include value="assert.request.numResources"
                   scope="common"
                   resource="Medication"
                   count="1"/>
   </test>
</TestScript>
